#include <bits/stdc++.h>
using namespace std;

int adj[1000][1000];
int visited[1000];

int bfs(int s, int n){
  queue<int>q;
  q.push(s);
  visited[s]=1;
  while(!q.empty()){
    int u=q.front();
    cout<<u<<" ";
    q.pop();
    for(int v=1; v<=n; v++){
     if(adj[u][v]==1 && visited[v]==0){
        q.push(v);
        visited[v]=1;
     }
    }
  }
}

int main(){
    int n,e;
    cin>>n>>e;
    for(int i=0; i<e; i++){
      int u,v;
      cin>>u>>v;
      adj[u][v]=1;
      adj[v][u]=1;
    }
    bfs(1,n);
}



