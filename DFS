#include<bits/stdc++.h>
using namespace std;
int adj[10000][10000];
int vis[1000000];

void dfs(int s,int n){
   vis[s]=1;
   cout<<s<<" ";
   for(int v=1; v<=n; v++){
      if(vis[v]==0 && adj[s][v]==1){
         dfs(v,n);
      }
   }
}

int main(){
   int n,e; cin>>n>>e;
   for(int i=0; i<e; i++){
      int u,v;
      cin>>u>>v;
      adj[u][v]=1;
      adj[v][u]=1;
   }

   dfs(1,n); 
}
