#include<bits/stdc++.h>
using namespace std;

vector<int>adj[50];
int visited[100];

void bfs(int n, int s){
   queue<int>q;
   q.push(s);
   visited[s]=1;
   while(!q.empty()){
    int u= q.front();
    cout<<u<<" ";
    q.pop();
    for(int i=0; i<adj[u].size(); i++){
        int v=adj[u][i];
        if(visited[v]==0){
            visited[v]=1;
            q.push(v);
        }
       }
   }
  
}

int main(){ 
    int n,e;
    cin>>n>>e;
    for(int i=0; i<e; i++){
        int u,v;
        cin>>u>>v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    } 
    bfs(n,1);
}
