#include<bits/stdc++.h>
using namespace std;

int main(){
   //taking input
   int n;
   cin>>n;
   int arr[n];
   for(int i=0; i<n; i++){
      cin>>arr[i];
   }

   //code for sorting 
   for(int i=0; i<n; i++){
      for(int j=i+1; j<n; j++){
         if(arr[i]>arr[j]){
            swap(arr[i],arr[j]);
         }
      }
   }

   // print the array
   for(int i=0; i<n; i++){
      cout<<arr[i]<<" ";
   }
}
