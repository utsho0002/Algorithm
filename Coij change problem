#include<bits/stdc++.h>
using namespace std;

int main(){
   int m,n;
   cin>>m>>n;
   int coin[m];
   for(int i=0; i<m; i++)cin>>coin[i];
   int dp[m+1][n+1];
   for(int i=0; i<m+1; i++){
      for(int j=0; j<n+1; j++){
         if(j==0){
            dp[i][j]=0;
         }
         else if(i==0){
            dp[i][j]=INT_MAX;
         }
         else if(coin[i-1]>j){
            dp[i][j]=dp[i-1][j];
         }
         else{
            dp[i][j]=min(dp[i-1][j], 1+dp[i][j-coin[i-1]]);
         }
      }
   }

   for(int i=0; i<m+1; i++){
      for(int j=0; j<n+1; j++){
         cout<<dp[i][j]<<" ";
      } cout<<endl;
   }

   cout<<"ans is: "<<dp[m][n];
}
